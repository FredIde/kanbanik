<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<ui:style>
		.mainPanel {
			
		}
		
		.boardPanel {
			float: left;
			border: 1px solid black;
		}
		
		.palettePanel {
			float: left;
			border: 1px solid pink;
		}
	</ui:style>

	<g:FlowPanel addStyleNames="mainPanel">
		<g:FlowPanel ui:field="board" addStyleNames="boardPanel" />
		<g:FlowPanel ui:field="palette" addStyleNames="palettePanel" />
	</g:FlowPanel>

</ui:UiBinder>